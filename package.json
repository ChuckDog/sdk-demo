{
  "name": "sdk-demo",
  "version": "1.0.0",
  "description": "This is a js sdk demo",
  "main": "index.js",
  "author": "Chuck",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "build:prod": "export NODE_ENV=production && rollup -c ./rollup.config.js",
    "build:dev": "export NODE_ENV=development && rollup -c ./rollup.config.js",
    "build:analyze": "export BUNDLE_ANALYZE=true && npm run build"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^5.3.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.8.0",
    "rollup": "^2.56.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.5.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix src",
      "git add"
    ]
  }
}
